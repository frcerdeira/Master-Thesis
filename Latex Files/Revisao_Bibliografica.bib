  pages = {8279-8289},
  author = {Nikoubashman, Arash and Howard, Michael P.},
  file = {/home/cerdeira/Documents/Tese (State of the Art)/Nikoubashman and Howard - 2017 - Equilibrium Dynamics and Shear Rheology of Semifle.pdf}
}

